user:
	user_id, primary key
	username
	password
	email
	first name
	last name
	gender
	born year
	born month
	born day
	photo
	type
	timeJoin

friends:
	user_id, foreign key index
	friend_id
	(user_id, friend_id) is primary key

posts:
	post_id, primary key
	user_id, foreign key index
	content
	image
	timeStamp

likeTable:
	user_id
	post_id
	(user_id, post_id) is primary key